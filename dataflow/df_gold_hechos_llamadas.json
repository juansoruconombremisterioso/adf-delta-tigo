{
	"name": "df_gold_hechos_llamadas",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_llamadas_entel",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_fecha",
						"type": "DatasetReference"
					},
					"name": "fecha"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_numero",
						"type": "DatasetReference"
					},
					"name": "numero"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_radiobase",
						"type": "DatasetReference"
					},
					"name": "radiobase"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_registro",
						"type": "DatasetReference"
					},
					"name": "registro"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_servicio",
						"type": "DatasetReference"
					},
					"name": "servicio"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_hechos_llamadas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "lookup3"
				},
				{
					"name": "lookup4"
				},
				{
					"name": "lookup5"
				}
			],
			"scriptLines": [
				"source(output(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_a as decimal(18,0),",
				"          longitud_a as decimal(18,0),",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_b as decimal(18,0),",
				"          longitud_b as decimal(18,0),",
				"          fecha as timestamp,",
				"          duracion as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          id_fecha as integer,",
				"          fecha as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> fecha",
				"source(output(",
				"          id_numero as integer,",
				"          numero as string,",
				"          imei as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> numero",
				"source(output(",
				"          id_radiobase as integer,",
				"          radiobase as string,",
				"          celda as string,",
				"          latitud as decimal(18,0),",
				"          longitud as decimal(18,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> radiobase",
				"source(output(",
				"          id_registro as integer,",
				"          registro as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> registro",
				"source(output(",
				"          id_servicio as integer,",
				"          servicio as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> servicio",
				"source1 select(mapColumn(",
				"          servicio,",
				"          registro,",
				"          numero_a,",
				"          rbs_utilizada_a,",
				"          rbs_utilizada_b,",
				"          fecha,",
				"          duracion,",
				"          numero_b",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1, fecha lookup(select1@fecha == fecha@fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, numero lookup(numero_b == numero",
				"     && numero_a == numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2, radiobase lookup(rbs_utilizada_a == radiobase",
				"     && rbs_utilizada_b == radiobase,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup3",
				"lookup3, registro lookup(select1@registro == registro@registro,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup4",
				"lookup4, servicio lookup(select1@servicio == servicio@servicio,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup5",
				"lookup5 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_llamada as integer,",
				"          duracion as timestamp,",
				"          id_servicio as integer,",
				"          id_registro as integer,",
				"          id_numera_a as integer,",
				"          id_radiobase_a as integer,",
				"          id_numero_b as integer,",
				"          id_radiobase_b as integer,",
				"          id_fecha as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}