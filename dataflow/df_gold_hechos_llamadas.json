{
	"name": "df_gold_hechos_llamadas",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_llamadas_entel",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_fecha",
						"type": "DatasetReference"
					},
					"name": "fecha"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_numero",
						"type": "DatasetReference"
					},
					"name": "numero"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_radiobase",
						"type": "DatasetReference"
					},
					"name": "radiobase"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_registro",
						"type": "DatasetReference"
					},
					"name": "registro"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_servicio",
						"type": "DatasetReference"
					},
					"name": "servicio"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_hechos_llamadas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "lookup3"
				},
				{
					"name": "lookup4"
				},
				{
					"name": "lookup5"
				},
				{
					"name": "lookup6"
				},
				{
					"name": "select2"
				},
				{
					"name": "select3"
				},
				{
					"name": "lookup7"
				}
			],
			"scriptLines": [
				"source(output(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_a as decimal(18,0),",
				"          longitud_a as decimal(18,0),",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_b as decimal(18,0),",
				"          longitud_b as decimal(18,0),",
				"          fecha as timestamp,",
				"          duracion as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          id_fecha as integer,",
				"          fecha as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT id_fecha, fecha FROM gold.dim_fecha',",
				"     format: 'query') ~> fecha",
				"source(output(",
				"          id_numero as integer,",
				"          numero as string,",
				"          imei as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT id_numero, numero, imei FROM gold.dim_numero',",
				"     format: 'query') ~> numero",
				"source(output(",
				"          id_radiobase as integer,",
				"          radiobase as string,",
				"          celda as string,",
				"          latitud as decimal(18,0),",
				"          longitud as decimal(18,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT id_radiobase, radiobase, celda, latitud, longitud FROM gold.dim_radiobase',",
				"     format: 'query') ~> radiobase",
				"source(output(",
				"          id_registro as integer,",
				"          registro as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT id_registro, registro FROM gold.dim_registro',",
				"     format: 'query') ~> registro",
				"source(output(",
				"          id_servicio as integer,",
				"          servicio as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT id_servicio, servicio FROM gold.dim_servicio',",
				"     format: 'query') ~> servicio",
				"source1 select(mapColumn(",
				"          servicio,",
				"          registro,",
				"          numero_a,",
				"          rbs_utilizada_a,",
				"          rbs_utilizada_b,",
				"          fecha,",
				"          duracion,",
				"          numero_b",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1, fecha lookup(select1@fecha == fecha@fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, numero lookup(numero_a == numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup6, radiobase lookup(rbs_utilizada_a == radiobase,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup3",
				"lookup7, registro lookup(select3@registro == registro@registro,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup4",
				"lookup4, servicio lookup(select3@servicio == servicio@servicio,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup5",
				"select2, numero lookup(numero_b == numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup6",
				"lookup2 select(mapColumn(",
				"          servicio,",
				"          registro,",
				"          numero_a,",
				"          rbs_utilizada_a,",
				"          rbs_utilizada_b,",
				"          fecha = select1@fecha,",
				"          duracion,",
				"          numero_b,",
				"          id_fecha,",
				"          fecha = fecha@fecha,",
				"          id_numero_a = id_numero",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"lookup3 select(mapColumn(",
				"          servicio,",
				"          registro,",
				"          numero_a,",
				"          rbs_utilizada_a,",
				"          rbs_utilizada_b,",
				"          fecha,",
				"          duracion,",
				"          numero_b,",
				"          id_fecha,",
				"          id_numero_a,",
				"          id_numero,",
				"          numero,",
				"          imei,",
				"          id_radiobase_a = id_radiobase",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"select3, radiobase lookup(rbs_utilizada_b == radiobase,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup7",
				"lookup5 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_llamada as integer,",
				"          duracion as timestamp,",
				"          id_servicio as integer,",
				"          id_registro as integer,",
				"          id_numera_a as integer,",
				"          id_radiobase_a as integer,",
				"          id_numero_b as integer,",
				"          id_radiobase_b as integer,",
				"          id_fecha as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          duracion,",
				"          id_servicio,",
				"          id_registro,",
				"          id_numera_a = id_numero_a,",
				"          id_radiobase_a,",
				"          id_numero_b = id_numero,",
				"          id_radiobase_b = id_radiobase,",
				"          id_fecha",
				"     )) ~> sink1"
			]
		}
	}
}