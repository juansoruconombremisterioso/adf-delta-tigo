{
	"name": "df_gold_dim_numero",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_datos_entel",
						"type": "DatasetReference"
					},
					"name": "numerosDatos"
				},
				{
					"dataset": {
						"referenceName": "ds_silver_llamadas_entel",
						"type": "DatasetReference"
					},
					"name": "numerosA"
				},
				{
					"dataset": {
						"referenceName": "ds_silver_llamadas_entel",
						"type": "DatasetReference"
					},
					"name": "numerosB"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "union1"
				},
				{
					"name": "union2"
				},
				{
					"name": "sort1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          numero as string,",
				"          imei as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT numero, imei FROM silver.datos',",
				"     format: 'query') ~> numerosDatos",
				"source(output(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_a as decimal(18,0),",
				"          longitud_a as decimal(18,0),",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_b as decimal(18,0),",
				"          longitud_b as decimal(18,0),",
				"          fecha as timestamp,",
				"          duracion as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT numero_a AS numero FROM silver.llamadas',",
				"     format: 'query') ~> numerosA",
				"source(output(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_a as decimal(18,0),",
				"          longitud_a as decimal(18,0),",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_b as decimal(18,0),",
				"          longitud_b as decimal(18,0),",
				"          fecha as timestamp,",
				"          duracion as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT numero_b AS numero FROM silver.llamadas',",
				"     format: 'query') ~> numerosB",
				"numerosDatos, numerosA union(byName: true)~> union1",
				"union1, numerosB union(byName: true)~> union2",
				"union2 sort(asc(numero, true)) ~> sort1",
				"sort1 aggregate(groupBy(numero),",
				"     columna1 = ERROR_FUNCTION(''),",
				"          each(match(name!='numero'), $$ = first($$))) ~> aggregate1"
			]
		}
	}
}